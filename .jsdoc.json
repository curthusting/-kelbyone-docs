{
  "recurseDepth": 10,
  "source": {
    "include": [
      "./_src/applications.js",
      "./_src/components/src",
      "package.json",
      "README.md"
    ],
    "includePattern": ".(js)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "light",
      "create_style": ".accordion {padding-left: 15px} .param-type dd {font-family: 'body', font-size: 100%}"
    }
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}